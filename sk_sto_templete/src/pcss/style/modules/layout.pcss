.wrap {
    position: relative;
    transition: .5s ease-in;
    z-index: 10;
    background-color: var(--color-bg-tranparent);
    &.wrap_white {
        background-color: #fff;
        & .contents {
            padding-top: 0;
            & .flow {
                height: 100px;
                & .title {
                    text-align: center;
                    font-family: var(--font);
        font-weight: 600;
                    font-size: 32px;
                    line-height: 1.5;
                    color: #6a75ca;
                    padding-top: 20px;
                }
                & .page_back {
                    position: absolute;
                    top : 30px;
                    right : 0;
                    width:40px;
                    height:40px;
                    border-radius:6px;
                    box-shadow:2px 2px 4px 0 white;
                    &:before,&:after{
                        content:'';
                        position:absolute;
                        width:42px;
                        height:2px;
                        background-color:#666;
                        border-radius:2px;
                        top:16px;
                    }
                    &:before{
                        -webkit-transform:rotate(45deg);
                        -moz-transform:rotate(45deg);
                        transform:rotate(45deg);
                        left:-1px;
                    }
                    &:after{
                        -webkit-transform:rotate(-45deg);
                        -moz-transform:rotate(-45deg);
                        transform:rotate(-45deg);
                        right:-1px;
                    }
                }





            }
        }
    }
}
.bg_contents {
    position: relative;
    width: 100%;
}

.bg_point {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 670px;
    background-color: var(--color-point);
    z-index: 1;
}
.contents {
    position: relative;
    width: 1200px;
    margin: 0 auto;
    //min-height: 700px;
    z-index: 5;
    padding: 60px 0 100px;
}


.text-guide {
    position: relative;
    font-size: 14px;
    font-family: var(--font-regular);

    &.text-guide-ml {
        margin-left: 20px;
    }

    & .point {
        color: var(--color-point);
    }

    & .fa-info-circle {
        position: absolute;
        left: 4px;
        top: 5px;
    }

    &.text-guide-pl {
        padding-left: 25px;
        font-size: 12px;
    }
}

.page_title {
    font-size: 26px;
    line-height: 36px;
    border-bottom: solid 1px #bdc9dd;
    padding-bottom: 14px;
    color: var(--color-text-dark);

    & i {
        margin: 0 7px 0 0;

        &.fa-chevron-right {
            margin: 0 7px;
            font-size: 20px;
            vertical-align: middle;
            margin-bottom: 5px;
        }
    }

    & span {
        color: var(--color-text-light);
    }
}

.info {
